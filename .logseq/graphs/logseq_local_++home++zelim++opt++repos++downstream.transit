    {:id #uuid \"655d6ffa-7db6-40d3-a030-50a919c2f88b\",\n               :page 33,\n               :position {:bounding {:x1 0,\n                                     :y1 254,\n                                     :x2 1104.5731201171875,\n                                     :y2 1408.984375,\n                                     :width 1305.6,\n                                     :height 1689.6},\n                          :rects ({:x1 0,\n                                   :y1 254,\n                                   :x2 0,\n                                   :y2 274,\n                                   :width 1305.6,\n                                   :height 1689.6}\n                                  {:x1 0,\n                                   :y1 270,\n                                   :x2 0,\n                                   :y2 290,\n                                   :width 1305.6,\n                                   :height 1689.6}\n                                  {:x1 0,\n                                   :y1 286,\n                                   :x2 0,\n                                   :y2 306,\n                                   :width 1305.6,\n                                   :height 1689.6}\n                                  {:x1 0,\n                                   :y1 302,\n                                   :x2 0,\n                                   :y2 322,\n                                   :width 1305.6,\n                                   :height 1689.6}\n                                  {:x1 0,\n                                   :y1 318,\n                                   :x2 0,\n                                   :y2 338,\n                                   :width 1305.6,\n                                   :height 1689.6}\n                                  {:x1 0,\n                                   :y1 334,\n                                   :x2 0,\n                                   :y2 354,\n                            