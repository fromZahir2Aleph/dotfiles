               :properties {:color \"yellow\"}}\n              {:id #uuid \"65fb31a7-d1ba-4a0c-96d5-532eda61452f\",\n               :page 189,\n               :position {:bounding {:x1 128.109375,\n                                     :y1 123.65625,\n                                     :x2 891.5146484375,\n                                     :y2 183.78125,\n                                     :width 1019.9999999999999,\n                                     :height 1319.9999999999998},\n                          :rects ({:x1 128.109375,\n                                   :y1 123.65625,\n                                   :x2 891.5146484375,\n                                   :y2 150.65625,\n                                   :width 1019.9999999999999,\n                                   :height 1319.9999999999998}\n                                  {:x1 128.109375,\n                                   :y1 156.78125,\n                                   :x2 426.89898681640625,\n                                   :y2 183.78125,\n                                   :width 1019.9999999999999,\n                                   :height 1319.9999999999998}),\n                          :page 189},\n               :content {:text \"full richness of the associated category, often revealing little if anything about its nature.\"},\n               :properties {:color \"yellow\"}}\n              {:id #uuid \"65fb33ab-c2a2-4b42-a0b1-3fd8ac3fef57\",\n               :page 189,\n               :position {:bounding {:x1 128.109375,\n                                     :y1 271.890625,\n                                     :x2 891.7797241210938,\n                                     :y2 464.15625,\n                                     :width 1019.9999999999999,\n                                     :height 1319.9999999999998},\n                          :rects ({:x1 164.921875,\n                                   :y1 271.890625,\n                                   :x2 891.779724121