,536870919]],["^15",[886,"^V",1236,536870919]],["^15",[886,"^U",1236,536870919]],["^15",[886,"^?",["^ ","^1W","annotation","^1X",43,"^1Y","yellow","^1?","6581e240-b1b6-4bdc-8264-622f61115dcc"],536870919]],["^15",[886,"^J",["^1W","^1X","^1Y","^1?"],536870919]],["^15",[886,"^4",["^ ","^1W","annotation","^1X","43","^1Y","yellow","^1?","6581e240-b1b6-4bdc-8264-622f61115dcc"],536870919]],["^15",[886,"^;","~u6581e240-b1b6-4bdc-8264-622f61115dcc",536870919]],["^15",[887,"^Q","N-Triples",536870919]],["^15",[887,"^O","^16",536870919]],["^15",[887,"^F",894,536870919]],["^15",[887,"^X",456,536870919]],["^15",[887,"^V",894,536870919]],["^15",[887,"^U",456,536870919]],["^15",[887,"^;","~u66ba0f9d-b88e-4dfe-8139-2211e6a2ef6a",536870919]],["^15",[888,"^Q","((655d6cfa-99c4-401d-b24a-0082c55805ed))",536870919]],["^15",[888,"^O","^16",536870919]],["^15",[888,"^F",948,536870919]],["^15",[888,"^X",456,536870919]],["^15",[888,"^V",974,536870919]],["^15",[888,"^U",456,536870919]],["^15",[888,"^H",939,536870919]],["^15",[888,"^;","~u66ba0f9d-25f0-419e-96fd-d4516fc9df62",536870919]],["^15",[889,"^Q","The Semantic Web standards are organized so that each language level builds on the one before so the languages themselves are layered. The following are the languages of the Semantic Web from least expressive to most expressive.\nls-type:: annotation\nhl-page:: 50\nhl-color:: yellow\nid:: 6581e7ec-a5a2-4cf5-8534-d0c565e156b2",536870919]],["^15",[889,"^O","^16",536870919]],["^15",[889,"^F",919,536870919]],["^15",[889,"^X",1236,536870919]],["^15",[889,"^V",1236,536870919]],["^15",[889,"^U",1236,536870919]],["^15",[889,"^?",["^ ","^1W","annotation","^1X",50,"^1Y","yellow","^1?","6581e7ec-a5a2-4cf5-8534-d0c565e156b2"],536870919]],["^15",[889,"^J",["^1W","^1X","^1Y","^1?"],536870919]],["^15",[889,"^4",["^ ","^1W","annotation","^1X","50","^1Y","yellow","^1?","6581e7ec-a5a2-4cf5-8534-d0c565e156b2"],536870919]],["^15",[889,"^;","~u6581e7ec-a5a2-4cf5-8534-d0c565e156b2",536870919]],["^15",[890,"^Q","((6