rofitable to share your data.\nls-type:: annotation\nhl-page:: 24\nhl-color:: yellow\nid:: 655d6cc3-7359-4078-8f9a-1528d7bd6b70",536870919]],["^15",[932,"^O","^16",536870919]],["^15",[932,"^F",1237,536870919]],["^15",[932,"^X",1236,536870919]],["^15",[932,"^V",1236,536870919]],["^15",[932,"^U",1236,536870919]],["^15",[932,"^?",["^ ","^1W","annotation","^1X",24,"^1Y","yellow","^1?","655d6cc3-7359-4078-8f9a-1528d7bd6b70"],536870919]],["^15",[932,"^J",["^1W","^1X","^1Y","^1?"],536870919]],["^15",[932,"^4",["^ ","^1W","annotation","^1X","24","^1Y","yellow","^1?","655d6cc3-7359-4078-8f9a-1528d7bd6b70"],536870919]],["^15",[932,"^;","~u655d6cc3-7359-4078-8f9a-1528d7bd6b70",536870919]],["^15",[933,"^Q","2.2 Explanation and Prediction",536870919]],["^15",[933,"^O","^16",536870919]],["^15",[933,"^F",950,536870919]],["^15",[933,"^X",456,536870919]],["^15",[933,"^V",872,536870919]],["^15",[933,"^U",456,536870919]],["^15",[933,"^;","~u66ba0f9d-31a4-49bb-87b6-28befb8c1d76",536870919]],["^15",[934,"^Q","",536870919]],["^15",[934,"^O","^16",536870919]],["^15",[934,"^F",881,536870919]],["^15",[934,"^X",456,536870919]],["^15",[934,"^V",957,536870919]],["^15",[934,"^U",456,536870919]],["^15",[934,"^;","~u66ba0f9d-b65c-4528-8e84-74e4128e9b56",536870919]],["^15",[935,"^Q","((65a16d56-1f79-4d99-a798-57965e6c325a))",536870919]],["^15",[935,"^O","^16",536870919]],["^15",[935,"^F",887,536870919]],["^15",[935,"^X",456,536870919]],["^15",[935,"^V",887,536870919]],["^15",[935,"^U",456,536870919]],["^15",[935,"^H",884,536870919]],["^15",[935,"^;","~u66ba0f9d-dc4a-4908-9bbe-563a97979f9d",536870919]],["^15",[936,"^Q","((655d6fa7-2f1d-44d1-9f9e-5eafada8fc00))",536870919]],["^15",[936,"^O","^16",536870919]],["^15",[936,"^F",924,536870919]],["^15",[936,"^X",456,536870919]],["^15",[936,"^V",924,536870919]],["^15",[936,"^U",456,536870919]],["^15",[936,"^H",896,536870919]],["^15",[936,"^;","~u66ba0f9d-39f4-4a85-bee0-5a0a86dad5fa",536870919]],["^15",[937,"^Q","((6581e4fa-982a-410a-b19b-30776216af98))((6