tom-query .table-auto>thead>tr>th {\n            background-color: rgba(0, 0, 0, 0.1);\n            padding: 3px 6px;\n        }\n        .custom-query .table-auto>tbody>tr>td.whitespace-nowrap {\n            overflow-wrap: break-word;\n            min-width: 20px;\n            white-space: normal;\n            font-weight: 300;\n            font-size: 13px;\n        }\n        .custom-query .table-auto>tbody>tr>.whitespace-nowrap img {\n            max-height: 120px;\n            margin: 0;\n        }\n/* ==================================== end of table resizer */\n\n/* ==========  BETTER-SIDEBAR - right sidebarshrinking panes w/ horizontal scroll v20221217==================*/\n\n        :root {\n            --sidebarItemMinWidth: 250px;\n            --sidebarItemMaxWidth: 950px;\n        }\n\n        .cp__right-sidebar-inner.flex.flex-col.h-full {\n            overflow: visible;\n        }\n\n        .sidebar-item-list {\n            width: fit-content;\n            height: 96vh !important;\n            overflow: unset;\n            display: flex;\n            flex-direction: column;\n            flex-wrap: wrap;\n            align-content: flex-start;\n            padding: 0 0 0 6px;\n        }\n\n        .cp__right-sidebar .sidebar-item {\n            display: inline-block;\n            align-self: flex-start;\n            overflow-y: auto;\n            min-height: 75px;\n            height: calc(100% - 10px) !important;\n            min-width: var(--sidebarItemMinWidth);\n            max-width: var(--sidebarItemMaxWidth);\n            width: 100%;\n            margin: 0px;\n            background-color: var(--ls-primary-background-color);\n            box-shadow: 1px 1px 5px rgba(0, 0, 0, 0.4);\n            border: 1px solid black;\n            border-radius: 10px;\n            resize: both;\n            padding: 1.5rem 0.5rem;\n        }\n\n        .cp__right-sidebar-inner .resizer {\n            position: absolute;\n            top: 0;\n            bottom: 0;\n            left: 0;\n            width: 6px;\n            user-select: none;\n            cursor: col-resize !important;\n            z-index: 10;\n        }\n\n        .cp__right-sidebar .resizer:hover {\n            background-color: var(--ph-highlight-strong);\n        }\n\n        .cp__right-sidebar-scrollable {\n            overflow-y: hidden;\n            overflow-x: auto;\n            height: 100%;\n        }\n\n        /* sliding panes */\n        .sidebar-item.folded {\n            display: inline-block;\n            overflow: hidden;\n            margin: 0px 0 !important;\n            padding: 5px;\n            resize: none;\n            width: 40px !important;\n            min-width: 40px;\n            writing-mode: vertical-LR;\n        }\n\n        .sidebar-item.folded .rotating-arrow.collapsed svg {\n            transform: rotate(180deg) translate(3px, -1.2rem);\n            transition: none;\n        }\n\n        .sidebar-item.folded .ml-1.font-medium {\n            margin-top: 2rem;\n            transition: none;\n        }\n\n        .cp__right-sidebar .sidebar-item.folded .close {\n            transform: scale(.8) translate(-8px);\n            transition: transform .1s;\n        }\n\n/* ======================================== end of BETTER-SIDEBAR =====*/\n/* ==== hide special props ==========================*/\n        div[data-refs-self=\"bg-pic\"] {\n            display: none !important;\n        }\n",536870918]],["^15",[20,"~:file/created-at",1723469721303,536870919]],["^15",[20,"^Z","logseq/custom.css",536870918]],["^15",[21,"^3","-",536870919]],["^15",[21,"^17",1723469716626,536870919]],["^15",[21,"^Z","journals/2024_08_08.md",536870919]],["^15",[22,"^K",1723469716612,536870919]],["^15",[22,"^[",21,536870919]],["^15",[22,"^N",20240808,536870919]],["^15",[22,"^@",true,536870919]],["^15",[22,"^Y","aug 8th, 2024",536870919]],["^15",[22,"^11","Aug 8th, 2024",536870919]],["^15",[22,"^B",1723469716612,536870919]],["^15",[22,"^;","~u66ba0f94-34